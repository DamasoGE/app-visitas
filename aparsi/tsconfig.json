{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "src/**/*"
  ],
  "extends": "expo/tsconfig.base"
}
